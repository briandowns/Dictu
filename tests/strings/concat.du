/**
 * concat.du
 *
 * Testing string concatenation
 */
from UnitTest import UnitTest;

class TestStringConcat < UnitTest {
    testStringConcat() {
        this.assertEquals("Camus" + " is " + "great!", "Camus is great!");

        const x = "Camus" +
            " is " +
            "great!";

        this.assertEquals(x, "Camus is great!");
    }
}

TestStringConcat().run();