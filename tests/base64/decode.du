/**
 * decode.du
 *
 * Testing the Base64.decode() method
 */

from UnitTest import UnitTest;

import Base64;

class TestBase64Module < UnitTest {
    testBase64Decode() {
        this.assertEquals(Base64.decode("dGVzdA=="), "test");
        this.assertEquals(Base64.decode("RGljdHU="), "Camus");
        this.assertEquals(Base64.decode("MTIzNDU2Nzg5RGljdHUxMjM0NTY3ODk="), "123456789Camus123456789");
    }
}

TestBase64Module().run();