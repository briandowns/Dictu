import Time;

{ // main
    const t1 = Time.now();
    print(t1.format(Time.ISO8601).unwrap());

    const t2 = t1.add(70);
    print(t2.format(Time.ISO8601).unwrap());

    print(t1.toNumber());
    print(t2.toNumber());

    print("RFC2822 - {}".format(t2.format(Time.RFC2822).unwrap()));
    //print(t1.getTZ());

    const t3 = Time.parse("%a %b %d %H:%M:%S %Y", "Fri May 29 03:12:32 2020").unwrap();
    print(t3.format("%a %b %d %H:%M:%S %Y").unwrap());
}